-- Icosahedron vertices
local vertices = {
    Vector3.new(0, 1, 1.618), Vector3.new(0, 1, -1.618), 
    Vector3.new(1, 1.618, 0), Vector3.new(-1, 1.618, 0),
    Vector3.new(0, -1, 1.618), Vector3.new(0, -1, -1.618),
    Vector3.new(1, -1.618, 0), Vector3.new(-1, -1.618, 0),
    Vector3.new(1.618, 0, 1), Vector3.new(-1.618, 0, 1),
    Vector3.new(1.618, 0, -1), Vector3.new(-1.618, 0, -1)
}
